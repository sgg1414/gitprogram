    <div class="zhan"></div>

    <div class="container">
        <div class="blue"></div>
        <div class="white">
            <div class="people">
                <img src="../images/nopeople.jpg" alt="" class="avatar">
            </div>
            <form action="/setmine" method="post" class="msg" enctype="multipart/form-data">
                昵称：<input type="text" name="name" value="<%=uname%>"><br> 性别：
                <input type="radio" name="sex" value="女" id="nv"><label for="nv">女</label> <input type="radio"
                    name="sex" value="男" id="nan"><label for="nan">男</label> <br> 简介：
                <textarea name="brief" id="" cols="80" rows="4"></textarea> <br> 上传头像：
                <input type="file" name="files" required>
                <button type="submit" class="btn btn-info">保存</button>
                <a href="/mine"> <button type="button" class="btn btn-info">取消</button></a>
            </form>
        </div>
    </div>
    <%if(msg){%>
        <script>
            confirm('修改失败，昵称已存在')
        </script>
